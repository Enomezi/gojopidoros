#include <iostream>
#include <vector>
#include <string>
using namespace std;
void matrix() {
    string matrix[7][7];
    for (int i = 0; i < 7; i++) {
        for (int j = 0; j < 7; j++) {
            matrix[i][j] = ".";
        }
    }
    for (int i = 0; i < 7; i++) {
        for (int j = 0; j < 7; j++) {
            cout << matrix[i][j] << " ";
        }
        cout << endl;
    }
}

int main() {
    matrix();
}
